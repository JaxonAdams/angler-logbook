{
    "version": 2,
    "builds": [
       { "src": "./server/server.js", "use": "@vercel/node" },
       { "src": "./client/build", "use": "@vercel/static" }
    ],
    "rewrites": [
        {
            "source": "/dashboard",
            "destination": "/client/build/index.html"
        },
        {
          "source": "/(.*)",
          "destination": "/server/server.js"
        }
    ]
}
